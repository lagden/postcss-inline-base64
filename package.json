{
  "name": "postcss-inline-base64",
  "version": "6.1.0",
  "description": "PostCSS plugin for encode the file to base64",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "base64",
    "file",
    "inline",
    "postcss-inline-base64"
  ],
  "author": "Thiago Lagden <lagden@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/lagden/postcss-inline-base64",
  "repository": "lagden/postcss-inline-base64",
  "bugs": {
    "url": "https://github.com/lagden/postcss-inline-base64/issues"
  },
  "main": "src/index.js",
  "files": [
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=12.9.0"
  },
  "scripts": {
    "precoverage": "npm run pretest",
    "coverage": "nyc --reporter=text-summary --reporter=lcov ava",
    "pretest": "xo",
    "test": "ava",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "xo": {
    "envs": [
      "node",
      "es6"
    ],
    "semicolon": false,
    "space": false,
    "rules": {
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "debug": "4.1.1",
    "file-type": "15.0.1",
    "got": "11.6.2",
    "is-svg": "4.2.1",
    "supports-color": "7.2.0"
  },
  "devDependencies": {
    "ava": "3.12.1",
    "nyc": "15.1.0",
    "snyk": "1.398.1",
    "xo": "0.33.1",
    "postcss": "8.0.3"
  },
  "peerDependencies": {
    "postcss": ">=8.0.0"
  },
  "snyk": true
}
